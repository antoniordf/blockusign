{
    "compilerOptions": {
      "target": "es6",
      "module": "ES2022",
      "moduleResolution": "node",
      "outDir": "./dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  